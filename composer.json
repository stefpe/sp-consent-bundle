{
    "name": "stefpe/sp-consent-bundle",
    "description": "A Symfony bundle that provides a GDPR-compliant cookie and consent banner component.",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "bundle", "gdpr", "cookie", "consent", "privacy", "cookie-banner"],
    "homepage": "https://github.com/stefpe/sp-consent-bundle",
    "authors": [
        {
            "name": "Stefan PÃ¶ltl",
            "email": "stefan.poeltl@example.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/http-foundation": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/ux-live-component": "^2.0",
        "symfony/twig-bundle": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "symfony/framework-bundle": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Stefpe\\SpConsentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stefpe\\SpConsentBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
